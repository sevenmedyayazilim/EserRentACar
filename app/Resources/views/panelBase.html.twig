{% endblock %}


{% block javascript %}

{% endblock %}


<script>
    $().ready(function () {
        $(":input").inputmask();
    });
    function BlokPage(tip) {
        if(tip=='enabled'){
            $('.blockpage').css('display','block');
        }else{
            $('.blockpage').css('display','none');
        }
    }
    function Alert(renk,body) {
        $('.toast').css('display','block');
        if (renk=='success') { $('.toast #renk').attr('fill','#00c292'); $('.toast').css('border', '2px solid #00c292'); $('.toast #title').html('Başarılı');}
        else if (renk=='warning') { $('.toast #renk').attr('fill','#fec107'); $('.toast').css('border', '2px solid #fec107'); $('.toast #title').html('Uyarı');}
        else if (renk=='danger') { $('.toast #renk').attr('fill','#e46a76'); $('.toast').css('border', '2px solid #e46a76'); $('.toast #title').html('Hata');}
        else if (renk=='info') { $('.toast #renk').attr('fill','#03a9f3'); $('.toast').css('border', '2px solid ##03a9f3'); $('.toast #title').html('Bilgi');}

        $('.toast #body').html(body);
        $('.toast').toast('show');
        setTimeout(function () {
            $('.toast').css('display','none');
        },4500);
    }


    function checkEmail(email) {
        var filter = /^[_a-z0-9-]+(\.[_a-z0-9-]+)*@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,4})$/;
        if (!filter.test(email)) {
            //alert('Lütfen uygun formatta bir mail adresi giriniz !');
            return false;
        }
        else{
            //alert('Mail adresi uygun formattadır.');
            return true;
        }
    }
    function checkUsername(username) {
        var filter = /^[a-zA-Z]{1}|[a-zA-Z0-9_\.\-]{4, 19}$/;
        if (!filter.test(username)) {
            //alert('Lütfen uygun formatta bir mail adresi giriniz !');
            return false;
        }
        else{
            //alert('Mail adresi uygun formattadır.');
            return true;
        }
    }
</script>
