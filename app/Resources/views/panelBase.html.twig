<div class="toast" data-autohide="true" data-delay="4000" style="position: fixed; top: 80px; right: 10px; z-index: 999999999999; width: 250px; display: none;">
    <div class="toast-header">
        <svg class="bd-placeholder-img rounded mr-2" width="20" height="20" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img"><rect fill="" id="renk" width="100%" height="100%"></rect></svg>
        <strong class="mr-auto" id="title"></strong>
        <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="toast-body" id="body">

    </div>
</div>
<div class="blockpage" style=" width: 100%; height: 100%; position: fixed; z-index: 99999999; display: none; top:0;left: 0;"></div>

